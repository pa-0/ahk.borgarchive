<NotepadPlus>
    <UserLang name="TableList" ext="tl tls" udlVersion="2.1">
        <Settings>
            <Global caseIgnored="yes" allowFoldOfComments="no" foldCompact="no" forcePureLC="2" decimalSeparator="0" />
				<!-- *gdb - Keywords 4 (mod column names) are prefix mode so we can highlight them to match the model row -->
            <Prefix Keywords1="no" Keywords2="no" Keywords3="no" Keywords4="yes" Keywords5="no" Keywords6="no" Keywords7="no" Keywords8="no" />
        </Settings>
        <KeywordLists>
				<!-- *gdb - Ignore lines -->
            <Keywords name="Comments">00; 01 02 03 04</Keywords>
            <Keywords name="Numbers, prefix1"></Keywords>
            <Keywords name="Numbers, prefix2"></Keywords>
            <Keywords name="Numbers, extras1"></Keywords>
            <Keywords name="Numbers, extras2"></Keywords>
            <Keywords name="Numbers, suffix1"></Keywords>
            <Keywords name="Numbers, suffix2"></Keywords>
            <Keywords name="Numbers, range"></Keywords>
				<!-- *gdb - Normal operators -->
            <Keywords name="Operators1">. ( )</Keywords>
				<!-- *gdb - Operators that require space around them -->
            <Keywords name="Operators2">- |</Keywords>
            <Keywords name="Folders in code1, open"></Keywords>
            <Keywords name="Folders in code1, middle"></Keywords>
            <Keywords name="Folders in code1, close"></Keywords>
            <Keywords name="Folders in code2, open">{</Keywords>
            <Keywords name="Folders in code2, middle"></Keywords>
            <Keywords name="Folders in code2, close">}</Keywords>
            <Keywords name="Folders in comment, open"></Keywords>
            <Keywords name="Folders in comment, middle"></Keywords>
            <Keywords name="Folders in comment, close"></Keywords>
				<!-- *gdb - Mod operations, from TableListMod.Operation_* -->
            <Keywords name="Keywords1">{{MOD_OPERATIONS}}</Keywords>
				<!-- *gdb - New column character, from SelectorGui.Char_NewColumn -->
            <Keywords name="Keywords2">!</Keywords>
				<!-- *gdb - Settings, from Selector -->
            <Keywords name="Keywords3">{{SETTINGS}}</Keywords>
				<!-- *gdb - Prefix for mod column identifiers, from TableListMod.Char_TargetPrefix -->
            <Keywords name="Keywords4">~</Keywords>
            <Keywords name="Keywords5"></Keywords>
            <Keywords name="Keywords6"></Keywords>
            <Keywords name="Keywords7"></Keywords>
            <Keywords name="Keywords8"></Keywords>
				<!-- *gdb - Delimiters:
					1: Model row (TableList.Char_Model_*)
					2: Column info row (TableList.Char_ColumnInfo_*) - &#x0009; is a tab, needed to keep this from bleeding into other areas of the file
					3: XML-style "tags" for replacement
					4: Headers
					5: URL case to avoid header-ing them
				-->
            <Keywords name="Delimiters">00[&#x0009; 01 02] 03(&#x0009; 04 05) 06&lt; 07 08&gt; 09# 10 11((EOL)) 12:// 13 14((EOL)) 15 16 17 18 19 20 21 22 23</Keywords>
        </KeywordLists>
        <Styles>
            <WordsStyle name="DEFAULT"           fgColor="EDEDE4" bgColor="2A211C" fontName="" fontStyle="0" nesting="0"    />
            <WordsStyle name="COMMENTS"          fgColor="000000" bgColor="2A211C" fontName="" fontStyle="0" nesting="0"    />
            <WordsStyle name="LINE COMMENTS"     fgColor="0080C0" bgColor="2A211C" fontName="" fontStyle="0" nesting="0"    />
            <WordsStyle name="NUMBERS"           fgColor="EDEDE4" bgColor="2A211C" fontName="" fontStyle="0" nesting="0"    />
            <WordsStyle name="KEYWORDS1"         fgColor="A3BFCD" bgColor="2A211C" fontName="" fontStyle="0" nesting="0"    />
            <WordsStyle name="KEYWORDS2"         fgColor="FF00FF" bgColor="2A211C" fontName="" fontStyle="1" nesting="0"    />
            <WordsStyle name="KEYWORDS3"         fgColor="8080C0" bgColor="2A211C" fontName="" fontStyle="0" nesting="0"    />
            <WordsStyle name="KEYWORDS4"         fgColor="FFFF80" bgColor="2A211C" fontName="" fontStyle="0" nesting="0"    />
            <WordsStyle name="KEYWORDS5"         fgColor="000000" bgColor="2A211C" fontName="" fontStyle="0" nesting="0"    />
            <WordsStyle name="KEYWORDS6"         fgColor="000000" bgColor="2A211C" fontName="" fontStyle="0" nesting="0"    />
            <WordsStyle name="KEYWORDS7"         fgColor="000000" bgColor="2A211C" fontName="" fontStyle="0" nesting="0"    />
            <WordsStyle name="KEYWORDS8"         fgColor="000000" bgColor="2A211C" fontName="" fontStyle="0" nesting="0"    />
            <WordsStyle name="OPERATORS"         fgColor="808080" bgColor="2A211C" fontName="" fontStyle="0" nesting="0"    />
            <WordsStyle name="FOLDER IN CODE1"   fgColor="000000" bgColor="2A211C" fontName="" fontStyle="0" nesting="0"    />
            <WordsStyle name="FOLDER IN CODE2"   fgColor="00FF80" bgColor="2A211C" fontName="" fontStyle="1" nesting="0"    />
            <WordsStyle name="FOLDER IN COMMENT" fgColor="008000" bgColor="2A211C" fontName="" fontStyle="0" nesting="0"    />
            <WordsStyle name="DELIMITERS1"       fgColor="FFFF80" bgColor="2A211C" fontName="" fontStyle="1" nesting="0"    />
            <WordsStyle name="DELIMITERS2"       fgColor="FFFF80" bgColor="2A211C" fontName="" fontStyle="0" nesting="0"    />
            <WordsStyle name="DELIMITERS3"       fgColor="80FF80" bgColor="2A211C" fontName="" fontStyle="0" nesting="0"    />
            <WordsStyle name="DELIMITERS4"       fgColor="0080FF" bgColor="2A211C" fontName="" fontStyle="1" nesting="2048" /><!-- *gdb - Nesting: keyword 2 (new column character) -->
				<WordsStyle name="DELIMITERS5"       fgColor="EDEDE4" bgColor="2A211C" fontName="" fontStyle="0" nesting="4"    /><!-- *gdb - Nesting: delimiter 3 (replacement tags) -->
            <WordsStyle name="DELIMITERS6"       fgColor="000000" bgColor="2A211C" fontName="" fontStyle="0" nesting="0"    />
            <WordsStyle name="DELIMITERS7"       fgColor="000000" bgColor="2A211C" fontName="" fontStyle="0" nesting="0"    />
            <WordsStyle name="DELIMITERS8"       fgColor="000000" bgColor="2A211C" fontName="" fontStyle="0" nesting="0"    />
        </Styles>
    </UserLang>
</NotepadPlus>
